{
  "name": "@cajacko/src",
  "version": "0.1.0",
  "private": true,
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "prepare": "yarn run build",
    "start": "concurrently \"yarn build:web -w\" \"yarn build:app -w\" \"yarn build:functions -w\"",
    "build": "concurrently \"yarn build:web\" \"yarn build:app\" \"yarn build:functions\"",
    "build:web": "tsc --p tsconfig.web.json",
    "build:app": "tsc --p tsconfig.app.json",
    "build:functions": "tsc --p tsconfig.functions.json"
  },
  "files": ["lib"],
  "devDependencies": {
    "concurrently": "^4.1.0",
    "typescript": "3.2.2"
  }
}
