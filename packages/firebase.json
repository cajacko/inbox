{
  "functions": {
    "predeploy": ["npm --prefix \"$RESOURCE_DIR\" run build"]
  },
  "hosting": {
    "public": "web/build",
    "rewrites": [
      {
        "source": "/api/**",
        "function": "graphql"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
